# Change the prompt shown in beacon console output to match
# [host] user/pid> 
# bid, from, text, when
set BEACON_INPUT {
    $bd = bdata($1);
    if ($2 eq mynick()) {
        return "\U[" . $bd['computer'] . "] " . $bd['user'] . "/" . $bd['pid'] . "\o> $3";
    } else {
        return "$2 \U[" . $bd['computer'] . "] " . $bd['user'] . "/" . $bd['pid'] . "\o> $3";
    }
}
